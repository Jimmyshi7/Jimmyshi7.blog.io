      <category domain="http://yoursite.com/tags/%E5%8F%8D%E5%B8%B8%E7%A7%AF%E5%88%86/">反常积分</category>
      
      <category domain="http://yoursite.com/tags/%E6%AF%94%E8%BE%83%E5%88%A4%E5%88%AB%E6%B3%95/">比较判别法</category>
      
      
